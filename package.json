{
    "packageManager": "yarn@3.0.2",
    "private": true,
    "dependencies": {
        "@loadable/component": "^5.15.0",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@reduxjs/toolkit": "^1.6.1",
        "axios": "^0.21.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.15.4",
        "@babel/preset-env": "^7.15.4",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@cypress/code-coverage": "^3.9.10",
        "@cypress/react": "^5.9.4",
        "@cypress/webpack-dev-server": "^1.5.0",
        "@types/copy-webpack-plugin": "^8.0.1",
        "@types/express": "^4.17.13",
        "@types/json-server": "^0.14.4",
        "@types/loadable__component": "^5.13.4",
        "@types/mini-css-extract-plugin": "^2.2.0",
        "@types/node": "^16.7.10",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "@types/react-redux": "^7.1.18",
        "@types/react-router-dom": "^5.1.8",
        "@types/webpack-dev-server": "^4.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-istanbul": "^6.0.0",
        "chalk": "^4.1.2",
        "concurrently": "^6.2.1",
        "copy-webpack-plugin": "^9.0.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.2.0",
        "cypress": "^8.3.1",
        "express": "^4.17.1",
        "fork-ts-checker-webpack-plugin": "^6.3.3",
        "html-webpack-plugin": "^5.3.2",
        "incstr": "^1.2.3",
        "json-server": "^0.16.3",
        "lodash": "^4.17.21",
        "mini-css-extract-plugin": "^2.2.2",
        "nyc": "^15.1.0",
        "prettier": "^2.3.2",
        "regenerator-runtime": "^0.13.9",
        "sass": "^1.39.0",
        "sass-loader": "^12.1.0",
        "speed-measure-webpack-plugin": "^1.5.0",
        "style-loader": "^3.2.1",
        "terser-webpack-plugin": "^5.2.3",
        "ts-node": "^10.2.1",
        "typedoc": "^0.21.9",
        "typescript": "~4.4.2",
        "wait-on": "^6.0.0",
        "webpack": "^5.52.0",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "4.1.0",
        "webpack-manifest-plugin": "^4.0.2"
    },
    "engines": {
        "node": ">=14.16.0"
    },
    "scripts": {
        "postinstall": "ts-node ./scripts/clearCache.ts",
        "build:dev": "webpack --mode development",
        "build:docs": "typedoc",
        "build:prod": "webpack --mode production",
        "chk": "prettier --check .",
        "fmt": "prettier --write .",
        "serve:db": "ts-node ./server/index.ts",
        "serve:dev:base": "ts-node ./scripts/clearCoverage.ts && concurrently --kill-others --names webpack,database,tests --prefix-colors blue,green,magenta --success first \"webpack serve --mode development\" \"ts-node ./server/index.ts\"",
        "serve:dev:tdd": "ts-node ./scripts/clearCoverage.ts && concurrently --kill-others --names webpack,database,tests --prefix-colors blue,green,magenta --success first \"webpack serve --mode development\" \"ts-node ./server/index.ts\" \"cypress open-ct\"",
        "serve:wds": "webpack serve --mode development",
        "test:e2e:once": "ts-node ./scripts/clearCoverage.ts && concurrently --kill-others --names webpack,database,tests --prefix-colors blue,green,magenta --success first \"cross-env PORT=9030 webpack serve --mode development\" \"cross-env PORT=9333 ts-node ./server/index.ts\" \"wait-on tcp:9030 tcp:9333 && cypress run\"",
        "test:e2e:gui": "ts-node ./scripts/clearCoverage.ts && concurrently --kill-others --names webpack,database,tests --prefix-colors blue,green,magenta --success first \"cross-env PORT=9030 webpack serve --mode development\" \"cross-env PORT=9333 ts-node ./server/index.ts\" \"wait-on tcp:9030 tcp:9333 && cypress open\"",
        "test:unit:once": "ts-node ./scripts/clearCoverage.ts && cypress run-ct",
        "test:unit:gui": "ts-node ./scripts/clearCoverage.ts && cypress open-ct",
        "verify:coverage:for-e2e": "nyc report --check-coverage --lines 100",
        "verify:coverage:for-unit": "nyc report --check-coverage --lines 100"
    }
}
