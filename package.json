{
    "packageManager": "yarn@3.0.1",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4.12.3",
        "@reduxjs/toolkit": "^1.6.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@cypress/code-coverage": "^3.9.10",
        "@cypress/react": "^5.9.4",
        "@cypress/webpack-dev-server": "^1.5.0",
        "@types/copy-webpack-plugin": "^8.0.1",
        "@types/json-server": "^0.14.4",
        "@types/mini-css-extract-plugin": "^2.2.0",
        "@types/node": "^16.7.10",
        "@types/react": "^17.0.19",
        "@types/react-dom": "^17.0.9",
        "@types/react-redux": "^7.1.18",
        "@types/react-router-dom": "^5.1.8",
        "@types/terser-webpack-plugin": "^5.0.4",
        "@types/webpack": "^5.28.0",
        "@types/webpack-dev-server": "^4.1.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-istanbul": "^6.0.0",
        "chalk": "^4.1.2",
        "concurrently": "^6.2.1",
        "copy-webpack-plugin": "^9.0.1",
        "cross-env": "^7.0.3",
        "css-loader": "^6.2.0",
        "cypress": "^8.3.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.3.2",
        "incstr": "^1.2.3",
        "json-server": "^0.16.3",
        "lodash": "^4.17.21",
        "mini-css-extract-plugin": "^2.2.1",
        "prettier": "^2.3.2",
        "sass": "^1.38.2",
        "sass-loader": "^12.1.0",
        "speed-measure-webpack-plugin": "^1.5.0",
        "terser-webpack-plugin": "^5.2.0",
        "ts-loader": "^9.2.5",
        "ts-node": "^10.2.1",
        "typedoc": "^0.21.9",
        "typescript": "~4.3.5",
        "wait-on": "^6.0.0",
        "webpack": "^5.51.1",
        "webpack-cli": "^4.8.0",
        "webpack-dev-server": "4.1.0",
        "webpack-manifest-plugin": "^4.0.2"
    },
    "scripts": {
        "build:dev": "webpack --mode development",
        "build:docs": "typedoc",
        "build:prod": "webpack --mode production",
        "chk": "prettier --check .",
        "fmt": "prettier --write .",
        "serve:db": "ts-node ./server/index.ts",
        "serve:dev:base": "concurrently --kill-others --names database,webpack,tests --prefix-colors blue,green,magenta --success first \"yarn run serve:db\" \"yarn run serve:wds\"",
        "serve:dev:tdd": "concurrently --kill-others --names database,webpack,tests --prefix-colors blue,green,magenta --success first \"yarn run serve:db\" \"yarn run serve:wds\" \"yarn run test:unit:gui\"",
        "serve:wds": "webpack serve --mode development",
        "sys:test:serve:db": "cross-env PORT=3031 yarn run serve:db",
        "sys:test:serve:wds": "cross-env PORT=3030 yarn run serve:wds",
        "sys:test:wait:db": "wait-on http://localhost:3031",
        "sys:test:wait:wds": "wait-on http://localhost:3030",
        "test:e2e:once": "concurrently --kill-others --names database,webpack,tests --prefix-colors blue,green,magenta --success first \"yarn run sys:test:serve:db\" \"yarn run sys:test:wait:db && yarn run sys:test:serve:wds\" \"yarn run sys:test:wait:wds && yarn run cypress run\"",
        "test:e2e:gui": "concurrently --kill-others --names database,webpack,tests --prefix-colors blue,green,magenta --success first \"yarn run sys:test:serve:db\" \"yarn run sys:test:wait:db && yarn run sys:test:serve:wds\" \"yarn run sys:test:wait:wds && yarn run cypress open\"",
        "test:unit:once": "cypress run-ct",
        "test:unit:gui": "cypress open-ct"
    }
}
