{
    "exclude": ["./.yarn", "./node*", "./out"],
    "include": ["./**/*"],
    "compilerOptions": {
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "esnext",
        "paths": {
            "@/*": ["./src/*"]
        }
    }
}
